      <Name>Team.Phase</Name>
      <Members>
         <ScriptProperty>
            <Name>targetType</Name>
            <GetScriptBlock>@("","Agent","Agentless")[$this.target.type]</GetScriptBlock>
         </ScriptProperty>
         <ScriptProperty>
            <Name>stepCount</Name>
            <GetScriptBlock>$this.steps.count</GetScriptBlock>
         </ScriptProperty>
         <MemberSet>
            <Name>PSStandardMembers</Name>
            <Members>
               <PropertySet>
                  <Name>DefaultDisplayPropertySet</Name>
                  <ReferencedProperties>
                     <Name>name</Name>
