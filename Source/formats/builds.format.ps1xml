                        <PropertyName>type</PropertyName>
                     </ListItem>
                     <ListItem>
                        <PropertyName>data</PropertyName>
                     </ListItem>
                     <ListItem>
                        <PropertyName>downloadUrl</PropertyName>
                     </ListItem>
                  </ListItems>
               </ListEntry>
            </ListEntries>
         </ListControl>
      </View>
