         <Name>Team.Build.Artifact.ListView</Name>
         <ViewSelectedBy>
            <TypeName>Team.Build.Artifact</TypeName>
         </ViewSelectedBy>
         <ListControl>
            <ListEntries>
               <ListEntry>
                  <ListItems>
                     <ListItem>
                        <PropertyName>id</PropertyName>
                     </ListItem>
                     <ListItem>
                        <PropertyName>name</PropertyName>
                     </ListItem>
